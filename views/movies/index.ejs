<%- include('../partials/header') %>
<table id="list">
  <thead>
    <tr>
      <th>Team</th>
      <th>Spread</th>
      <th>Detials</th>
      <th>Delete Pick</th>
    </tr>
  </thead>
  <tbody>
    <% movies.forEach(function(m) { %>
      <tr>
        <td><%= m.title %></td>
        <td><%= m.releaseYear %></td>
        <td><a href="/movies/<%= m._id %>">PICK DETAILS</a></td>
        <td><form class="delete-form" action="/movies/<%= m._id %>?_method=DELETE" method="POST">
          <button type="submit">X</button>
      </form></td>
      </tr>
      <% }); %>
    </tbody>
  </table>
  
<%- include('../partials/footer') %>
